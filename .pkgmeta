package-as: ElvUI_EltreumUI

enable-nolib-creation: no

ignore:
  - eltruism/.git
  - eltruism/.github
  - .pkgmeta
  - eltruism/Changelog.md
  - eltruism/License.md
  - eltruism/README.md
  - "*.md"
  - eltruism/Screenshots
  - Screenshots
  - .luacheckrc

move-folders:
    ElvUI_EltreumUI/ElvUI_EltreumUI: ElvUI_EltreumUI

manual-changelog:
  filename: Latest.md
  markup-type: markdown
